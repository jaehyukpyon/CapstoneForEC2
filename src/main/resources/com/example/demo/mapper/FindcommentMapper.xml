<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.FindCommentMapper">


<insert id="saveComment" useGeneratedKeys="true" keyProperty="commentId">
        insert into FindComment (content, userId,findId,registerAt)
        values ( #{comment.content},#{comment.userId},#{findId},#{comment.registerAt})
    </insert>




</mapper>